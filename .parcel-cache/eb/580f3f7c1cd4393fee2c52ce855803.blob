    url = "https://" + url;
  }
  console.log(url);
  const $siteList = $(".siteList");
  const $lastList = $siteList.find("li.last");
  console.log($siteList);
  const $site = $(`<li>
  <a href="${url}">
        <div class="site">
            <div class="logo">${url[0]}</div>
            <div class="link">${url}</div>
        </div>
    </a>
  </li>`).insertBefore($lastList);
});
